			set {Kralovstvi_Hrad_ohen1} to 0
			command "zhasnuti -2970 105 -696 war"
			set {Kralovstvi_Hrad_ohen2} to 0
			command "zhasnuti -2970 105 -706 war"
			set {Kralovstvi_Hrad_ohen3} to 0
			command "zhasnuti -2969 105 -709 war"
			set {Kralovstvi_Hrad_ohen4} to 0
		string-arg 1 is "zabiti":
			if {Kralovstvi_Hrad.%server%} is between 0 and 49:
				command "zhasnuti -2969 105 -693 war"
				command "zhasnuti -2970 105 -696 war"
				command "zhasnuti -2970 105 -706 war"
				command "zhasnuti -2969 105 -709 war"
				set {Kralovstvi_Hrad_ohen1} to 0
				set {Kralovstvi_Hrad_ohen2} to 0
				set {Kralovstvi_Hrad_ohen3} to 0
				set {Kralovstvi_Hrad_ohen4} to 0
			if {Kralovstvi_Hrad.%server%} is between 50 and 99:
