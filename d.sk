command /hrad [<text>] [<num>]:
	permission: Kralovstvi_Hrad.admin
	permission message: &c&l[HRAD]&7&lYou do not have permission.
	executable by: player and console
	trigger:
		string-arg 1 is "":
			send "&aBylo zabito &c%{Kralovstvi_Hrad.%server%}%/200 &anemrtvych!"
		string-arg 1 is "start":
			if {Kralovstvi_Hrad.%server%} is between 0 and 199:
				set {Kralovstvi_Hrad.%server%} to 0
				send "&cBohuzel se vam nepovedlo zabit dostatecne mnozstvi prokletych." to all players
			if {Kralovstvi_Hrad.%server%} is more than 199:
				command "zazehnuti -2977 102 -698 war"
				command "zazehnuti -2977 102 -704 war"
				play raw resourcepack sound "podpaleni" to all players at {Kralovstvi_Hrad_lokacebrany1}
				wait 2 seconds
				command "zazehnuti -2981 100 -698 war"
				command "zazehnuti -2981 100 -704 war"
				play raw resourcepack sound "podpaleni" to all players at {Kralovstvi_Hrad_lokacebrany1}
				wait 2 seconds
				command "zazehnuti -2985 98 -698 war"
				command "zazehnuti -2985 98 -704 war"
				play raw resourcepack sound "podpaleni" to all players at {Kralovstvi_Hrad_lokacebrany1}
				wait 2 seconds
				command "zazehnuti -2989 96 -698 war"
				command "zazehnuti -2989 96 -704 war"
				play raw resourcepack sound "podpaleni" to all players at {Kralovstvi_Hrad_lokacebrany1}
				wait 2 seconds
				set blocks within {Kralovstvi_Hrad_lokacebrany1} and {Kralovstvi_Hrad_lokacebrany2} to air
				play raw resourcepack sound "brana" to all players at {Kralovstvi_Hrad_lokacebrany1}
				wait 1 second
				set blocks within {Kralovstvi_Hrad_lokacebrany3} and {Kralovstvi_Hrad_lokacebrany4} to air
				wait 2 seconds
				set {Kralovstvi_Hrad_teleportkral} to 1
				play raw resourcepack sound "kouzlox5" to all players at {Kralovstvi_Hrad_lokacebrany1}
				play raw resourcepack sound "kouzlox5" to all players at {Kralovstvi_Hrad_lokaceportalu1}
				wait 27 seconds
				play raw resourcepack sound "brana" to all players at {Kralovstvi_Hrad_lokacebrany1}
				set {Kralovstvi_Hrad_teleportkral} to 0
				set blocks within {Kralovstvi_Hrad_lokacebrany3} and {Kralovstvi_Hrad_lokacebrany4} to iron_bars
				wait 1 second
				set blocks within {Kralovstvi_Hrad_lokacebrany1} and {Kralovstvi_Hrad_lokacebrany2} to iron_bars
				command "mm m spawn Carodej 1 war,-2956,100,-700"
				send "&6&lObjevil se kral" to all players
				wait 3 seconds
				command "zhasnuti -2977 102 -698 war"
				command "zhasnuti -2977 102 -704 war"
				wait 3 seconds
				command "zhasnuti -2981 100 -698 war"
				command "zhasnuti -2981 100 -704 war"
				wait 3 seconds
				command "zhasnuti -2985 98 -698 war"
				command "zhasnuti -2985 98 -704 war"
				wait 3 seconds
				command "zhasnuti -2989 96 -698 war"
				command "zhasnuti -2989 96 -704 war"
				set {Kralovstvi_Hrad.%server%} to 0
				wait 10 seconds
